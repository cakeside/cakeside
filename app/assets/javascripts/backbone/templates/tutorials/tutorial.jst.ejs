<div class="thumbnail">
  <a href="#tutorials/<%= id %>">
    <img src="<%= image_url %>" />
  </a>
  <div class="caption">
    <h3><a href="#tutorials/<%= id %>"><%= heading %></a> <small><%= displayDate(created_at) %></small></h3>
    <p>
      <% _.each(tags, function(tag) { %> 
        <a href="">
          <span class="label"><%= tag.name %></span>
        </a>
      <% }); %>
    </p>
    <p><%= description %></p>
    <p><a href="#tutorials/<%= id %>" class="btn">View</a></p>
  </div>
</div>
